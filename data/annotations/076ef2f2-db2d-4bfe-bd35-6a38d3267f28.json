      "start_time": 205,
      "end_time": 206,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Ya se puede trabajar este barro"
            }
          ]
        }
      ],
      "tags": [],
      "uuid": "8b07d133-c3b2-42af-bb87-b2ea5aae6013"
    },
