      "end_time": 305,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Así lo despegamos bien"
            }
          ]
        }
      ],
      "tags": [],
      "uuid": "1661095b-90ae-48d5-ae50-38e6c687dcfa"
    },
    {
      "start_time": 305,
      "end_time": 308,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Despegamos todo bien"
            }
          ]
        }
      ],
      "tags": [],
      "uuid": "6351a362-d877-4126-b48d-6f6c8bc4e156"
    },
    {
