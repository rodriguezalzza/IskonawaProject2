      "start_time": 218,
      "end_time": 222,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Pero aun así nos dicen que los Iskonawa no tenemos cerámica"
            }
          ]
        }
      ],
      "tags": [],
      "uuid": "dc360bba-9f99-4c74-8148-6791e4f04c75"
    },
