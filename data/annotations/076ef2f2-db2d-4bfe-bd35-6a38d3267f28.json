      "start_time": 222,
      "end_time": 225,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Nosotros también tenemos cerámica"
            }
          ]
        }
      ],
      "tags": [],
      "uuid": "df68c6f6-665b-402f-b743-bbb65996fc34"
    },
