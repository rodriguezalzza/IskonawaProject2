      "end_time": 218,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Ellos cocinaban por más que nadie les ha enseñado"
            }
          ]
        }
      ],
      "tags": [],
      "uuid": "a4acd05d-56e4-48db-b50d-386367916f5c"
    },
